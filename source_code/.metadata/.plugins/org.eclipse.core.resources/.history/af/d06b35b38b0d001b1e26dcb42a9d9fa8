#include "uart.h"
#include "external_eeprom.h"
#define READY 0
int main(){
	//uint8 sent_pass[7];
	uint8 x[7],i;
	/* Init EEPROM */
	EEPROM_init();
	UART_init();
	while(1){
	for(i=0;i<5;i++){
	x[i]=UART_recieveByte();
	EEPROM_writeByte((0x000|i), x[i]);
	_delay_ms(100);
	}
	return 0;
}

}
