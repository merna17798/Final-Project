#include "uart.h"
#include "external_eeprom.h"
#define READY 256
int main(){
	//uint8 sent_pass[7];
	uint8 x[7],i;
	/* Init EEPROM */
	EEPROM_init();
	UART_init();
	//UART_sendByte(READY);
	for(i=0;i<=5;i++){
		UART_sendByte(READY);
		x[i]=UART_recieveByte();
		EEPROM_writeByte((0x0000|i), x[i]);
		_delay_ms(100);
	}
	while(1){



	}

	return 0;
}
